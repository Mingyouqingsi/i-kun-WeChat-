swiper {
  width: 100%;
  height: 400rpx;
}

swiper image {
  width: 100%;
  height: 400rpx;
}

.test-src {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0px;
  background: rgba(0, 0, 0, 0.4);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10;
}

.equityList {
  width: 750rpx;
  height: 82rpx;
  display: flex;
  border-bottom: 1rpx solid #bbb;
  border-top: 1rpx solid #bbb;
  background-color: #fff;
}

.listTitle {
  font-size: 24rpx;
  color: black;
  flex: 0.2;
  display: flex;
  justify-content: center;
  align-items: center;
}

.listTitle:not(:first-child) {
  border-left: 1rpx solid #bbb;
}

.listTitle.active {
  color: #df495b;
  position: relative;
}

.listTitle.active::after {
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  content: '';
  width: 100%;
  height: 18rpx;
  position: absolute;
  left: 0;
  bottom: -15rpx;
}

.eqImg {
  width: 100%;
  height: 400rpx;
  position: relative;
}

.where {
  position: absolute;
  top: 200rpx;
  left: 50%;
  transform: translateX(-50%);
  border: 1rpx #bbb solid;
  padding: 10rpx;
  color: yellow;
  /* z-index: 5; */
}
.iswhere {
  top: 300rpx;
}
.isthere {
  top: 100rpx;
}
.douyu-btn {
  position: absolute;
  top: 200rpx;
  left: 50%;
  transform: translateX(-50%);
  border: 1rpx #bbb solid;
}
